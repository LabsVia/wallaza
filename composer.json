{
    "name": "wallaza/api",
    "description": "Wallaza Wallpaper API - Access millions of wallpapers programmatically",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Wallaza Development Team",
            "email": "developers@wallaza.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^2.0",
        "vlucas/phpdotenv": "^5.3",
        "firebase/php-jwt": "^5.4",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Wallaza\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wallaza\\Api\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 